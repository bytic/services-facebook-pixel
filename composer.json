{
  "name": "bytic/facebook-pixel",
  "description": "Facebook Pixel interaction library",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "facebook",
    "pixel"
  ],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/ByTIC/services-facebook-pixel",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "facebook/php-business-sdk": "^22.0.1"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "symfony/http-kernel": ">=5.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\FacebookPixel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\FacebookPixel\\Tests\\": "tests/src",
      "ByTIC\\FacebookPixel\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
